{
  "extends": "next/core-web-vitals",
  "plugins": ["regex"],
  "rules": {
    "regex/invalid": [
      "error",
      [
        {
          "regex": "invalidRegex1",
          "replacement": "newValue"
        },
        {
          "id": "regexIdN",
          "message": "errorMessageN",
          "regex": "invalidRegexN",
          "files": {
            "ignore": "ignoreFilesRegexN"
          }
        }
      ]
    ]
  }
}
